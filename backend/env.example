# Prism Browser Backend Configuration

# Application
APP_DEBUG=true
APP_ENV=development

# Database
DB_DRIVER=sqlite
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=prism.db
DB_USERNAME=
DB_PASSWORD=

# Engines
DEFAULT_ENGINE=chromium
CHROMIUM_BINARY=/usr/bin/chromium-browser
FIREFOX_BINARY=/usr/bin/firefox

# API
API_BASE_URL=http://localhost:8000

# Logging
LOG_LEVEL=info
LOG_PATH=logs/app.log

# Security
ENCRYPTION_KEY=your-secret-key-here-change-in-production
